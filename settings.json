    "projectManager.sortList": "Path",
    "diffEditor.ignoreTrimWhitespace": false,
    "window.openFoldersInNewWindow": "on",
    "material-icon-theme.folders.color": "#90a4ae",
    "workbench.iconTheme": "material-icon-theme",
