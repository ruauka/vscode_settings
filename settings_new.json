{
    "go.toolsManagement.autoUpdate": true,
    "workbench.startupEditor": "none",
    "[python]": {
        "editor.formatOnType": true
    },
    "code-runner.executorMap": {
        "python": "cd $workspaceRoot && python -u main.py",
        "go": "cd $workspaceRoot && go run main.go",
        "rust": "cd $workspaceRoot && cargo run"
    },
    "files.autoSave": "afterDelay",
    "workbench.tree.indent": 15,
    "editor.fontSize": 12.5,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "name": "One Dark italic",
                "scope": [
                    "comment",
                    "entity.other.attribute-name",
                    "keyword",
                    "markup.underline.link",
                    "storage.modifier",
                    // "storage.type",
                    "string.url",
                    "variable.language.super",
                    "variable.language.this"
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "name": "One Dark italic reset",
                "scope": [
                    "keyword.operator",
                    "keyword.other.type",
                    "storage.modifier.import",
                    "storage.modifier.package",
                    "storage.type.built-in",
                    "storage.type.function.arrow",
                    "storage.type.generic",
                    "storage.type.java",
                    "storage.type.primitive",
                    "variable.parameter.go"
                ],
                "settings": {
                    "fontStyle": ""
                }
            },
            {
                "scope": [
                    "keyword.operator.assignment.go",
                ],
                "settings": {
                    // "foreground": "#9bcdd6",
                    "foreground": "#fff6df"
                }
            },
            {
                "scope": [
                    "support.type.property-name.json.comments",
                    "support.type.property-name.json",
                ],
                "settings": {
                    "foreground": "#3498DB"
                }
            },
        ]
    },
    "editor.semanticTokenColorCustomizations": {
        "[One Dark]": {
            "rules": {
                // go
                "variable:go": {
                    "foreground": "#cfccc8"
                    // "fontStyle": "italic"
                },
                "function.definition.signature:go": {
                    "foreground": "#4CCD87"
                    // "fontStyle": "italic"
                },
                "function.signature:go": {
                    "foreground": "#B09D79"
                    // "fontStyle": "italic"
                },
                "function.defaultLibrary:go": {
                    "foreground": "#CC7832"
                    // "fontStyle": "italic"
                },
                "method.definition.signature:go": {
                    "foreground": "#4CCD87"
                    // "fontStyle": "italic"
                },
                "method.signature:go": {
                    "foreground": "#B09D79"
                    // "fontStyle": "italic"
                },
                "namespace:go": {
                    "foreground": "#cdd4b2",
                    "fontStyle": "italic"
                },
                "parameter.definition.signature:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "parameter.pointer:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "parameter.string:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "parameter.number:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "parameter.bool:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "parameter.map:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "parameter.slice:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "parameter.interface:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "parameter.struct:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "parameter.definition.interface:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "parameter.definition.number:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "parameter.definition.slice:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "parameter.definition.string:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "parameter.definition.map:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "parameter.definition.bool:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "parameter.definition.struct:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "variable.readonly:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "number:go": {
                    "foreground": "#52B4D8",
                    // "fontStyle": "italic"
                },
                "type.definition.struct:go": {
                    "foreground": "#92D8E8",
                    // "fontStyle": "italic"
                },
                "type.struct:go": {
                    "foreground": "#a3cd75",
                    // "fontStyle": "italic"
                },
                "type.signature:go": {
                    "foreground": "#a3cd75",
                    // "fontStyle": "italic"
                },
                "type.interface:go": {
                    "foreground": "#a3cd75",
                    // "fontStyle": "italic"
                },
                "type.defaultLibrary.interface:go": {
                    "foreground": "#e3b854",
                    // "fontStyle": "italic"
                },
                "type.slice:go": {
                    "foreground": "#a3cd75",
                    // "fontStyle": "italic"
                },
                "keyword:go": {
                    "foreground": "#e3b854",
                    // "fontStyle": "italic"
                },
                "operator:go": {
                    "foreground": "#fff6df",
                    // "fontStyle": "italic"
                },
                "string.format:go": {
                    "foreground": "#56B6C2",
                    // "fontStyle": "italic"
                },
                // rust
                "property:rust": {
                    "foreground": "#ABB2BF"
                    // "fontStyle": "italic"
                },
                "property.declaration:rust": {
                    "foreground": "#ABB2BF"
                    // "fontStyle": "italic"
                },
                "property.library:rust": {
                    "foreground": "#ABB2BF"
                    // "fontStyle": "italic"
                },
                "formatSpecifier:rust": {
                    "foreground": "#56B6C2"
                    // "fontStyle": "italic"
                },
                // python
                "property:python": {
                    "foreground": "#ABB2BF"
                    // "fontStyle": "italic"
                },
                "property.declaration:python": {
                    "foreground": "#ABB2BF"
                    // "fontStyle": "italic"
                },
                "property.library:python": {
                    "foreground": "#ABB2BF"
                    // "fontStyle": "italic"
                },
            }
        }
    },
    "workbench.colorTheme": "One Dark",
    // "workbench.colorTheme": "Cold Python Theme",
    "makefile.configureOnOpen": true,
    "remote.SSH.enableDynamicForwarding": false,
    "editor.largeFileOptimizations": false,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "lldb.suppressUpdateNotifications": true,
    "explorer.confirmPasteNative": false,
    // "go.gopath": "/home/user/go",
    // "go.goroot": "/usr/local/go1.20",
    // "go.useLanguageServer": true
    "go.alternateTools": {},
    "docker.extension.dockerEngineAvailabilityPrompt": false,
    "editor.fontFamily": "Monaco",
    "terminal.integrated.fontFamily": "Monaco",
    "rust-analyzer.workspace.discoverConfig": null,
    "workbench.colorCustomizations": {
